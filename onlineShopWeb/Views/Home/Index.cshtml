@model onlineShopWeb.Models.HomeViewModel

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@foreach (var item in Model.BooksList)
{
    @Html.ActionLink(item.Name, "Details", onlineShopWeb.Views.Controllers.Books, new { id = item.Id}, null) 
    @item.Price<br/>
}


@foreach (var item in Model.SongsList)
{
    @Html.ActionLink(item.Name, "Details", onlineShopWeb.Views.Controllers.Songs, new { id = item.Id }, null)
    @item.Price<br/>
}
