@model onlineShopWeb.Models.HomeViewModel

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@foreach (var item in Model.BooksList)
{
    @Html.ActionLink(item.Name, "Details", "Book", new { id=item.Name}, null) 
    @item.Price<br/>
}


@foreach (var item in Model.SongsList)
{
    @Html.ActionLink(item.Name, "Details", "Song", new { id = item.Name }, null)
    @item.Price<br/>
}
